{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.fixAll.eslint": "explicit"
    },
    "prettier.requireConfig": true,
    "files.associations": {
        "*.jsx": "javascriptreact"
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/dist": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true
    }
}